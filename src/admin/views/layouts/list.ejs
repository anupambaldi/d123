<!DOCTYPE html>
<html lang="zxx">

<head>
   <!-- Page Title -->
   <title><%= siteTitle %></title>

   <!-- Meta Data -->
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta http-equiv="content-type" content="text/html; charset=utf-8">
   <meta name="viewport" content="width=device-width, initial-scale=1">
   <meta name="description" content="">
   <meta name="keywords" content="">

   <!-- Favicon -->
   <link rel="shortcut icon" href="<%= siteUrl %>/accessory.png">

   <!-- Web Fonts -->
   <link href="https://fonts.googleapis.com/css?family=PT+Sans:400,400i,700,700i&display=swap" rel="stylesheet">
   
   <!-- ======= BEGIN GLOBAL MANDATORY STYLES ======= -->
   <link rel="stylesheet" href="<%= siteUrl %>/bootstrap/css/bootstrap.min.css">
   <link href="<%= siteUrl %>/css/icons.min.css" rel="stylesheet" type="text/css">
   <link rel="stylesheet" href="<%= siteUrl %>/fonts/icofont/icofont.min.css">
   <link rel="stylesheet" href=".<%= siteUrl %>/plugins/perfect-scrollbar/perfect-scrollbar.min.css">
   <link rel="stylesheet" href="<%= siteUrl %>/plugins/apex/apexcharts.css">
   <!-- ======= END BEGIN GLOBAL MANDATORY STYLES ======= -->

   <!-- ======= MAIN STYLES ======= -->
   <link rel="stylesheet" href="<%= siteUrl %>/css/style.css">
   <link href="<%= siteUrl %>/libs/flatpickr/flatpickr.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/bootstrap-datepicker/bootstrap-datepicker.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/summernote/summernote-bs4.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/select2/select2.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/jquery-nice-select/nice-select.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/bootstrap-select/bootstrap-select.min.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/bootstrap-touchspin/jquery.bootstrap-touchspin.css" rel="stylesheet" type="text/css">
    <link href="<%= siteUrl %>/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet" type="text/css">
   <link href="<%= siteUrl %>/libs/cropper/cropper.css"  rel="stylesheet">
    <script src="<%= siteUrl %>/js/jquery.min.js"></script>
   <!-- ======= END MAIN STYLES ======= -->
</head>
<body>
    <!-- Offcanval Overlay -->
    <div class="offcanvas-overlay"></div>
    <div id="wrapper">
    <% include ../layouts/partials/topBar %>
    <!-- Main Wrapper -->
    <div class="main-wrapper">
        <% include ../layouts/partials/leftSideBar %>
        <div class="main-content">
            <div class="container-fluid">
                <% if (errorFlash || successFlash || infoFlash) { %>
                    <div class="row">
                        <div class="col-md-12" style="margin-top: 15px">
                            <% if (errorFlash){ %>
                                <div class="alert alert-danger alert-dismissible fade show" role="alert">
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                        <span aria-hidden="true">×</span>
                                    </button>
                                    <%= errorFlash %>
                                </div>
                            <% } if (successFlash){ %>
                                <div class="alert alert-success alert-dismissible fade show" role="alert">
                                    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                        <span aria-hidden="true">×</span>
                                    </button>
                                    <%= successFlash %>
                                </div>
                            <% } if (infoFlash){ %>
                                    <div class="alert alert-info alert-dismissible fade show" role="alert">
                                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                            <span aria-hidden="true">×</span>
                                        </button>
                                        <%= infoFlash %>
                                    </div>
                            <% } %>
                        </div>
                    </div>
                <% } %>
                <%- body %>
            </div>
        </div>
    </div>
    <% include ../layouts/partials/footer %>
</div>
<script src="<%= siteUrl %>/js/script.js"></script>

