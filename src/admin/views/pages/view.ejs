<% layout('../layouts/admin') -%>
<div class="row ">
    <div class="col-md-12">
        <div class="card bg-transparent">
            <div class="contact-header d-flex align-items-sm-center media flex-column flex-sm-row bg-white mb-30">
                <div class="contact-header-left media-body d-flex align-items-center mr-4 col-6">
                    <!-- Add New Contact Btn -->
                    <div class="add-new-contact mr-20">
                        <div class="float-left">
                            <ol class="breadcrumb m-0">
                                <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
                                <li class="breadcrumb-item"><a href="/pages">Static Pages</a></li>
                                <li class="breadcrumb-item active"><a href="javascript: void(0);">View</a></li>
                            </ol>
                        </div>
                    </div>
                </div>
               
            </div>
         </div>
        
    </div>
</div>
<div class="row">
    <div class="col-12">
        <div class="card mb-30">
            <h4 class="page-title ml-20 mt-30">View Static Page</h4>
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group mb-3">
                            <label>Title: </label> <%= page.title %>
                        </div>
                    </div>
                    <div class="col-lg-12">
                        <div class="form-group mb-3">
                            <label>Description:</label>
                            <%- page.description %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
